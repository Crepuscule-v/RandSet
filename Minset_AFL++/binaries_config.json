{
    "nm-new": {
        "AFL_CFG_PATH": "./fuzz_target_binaries/nm_sancov_cfg",
        "FUZZ_TARGET": "./fuzz_target_binaries/nm-new",
        "FUZZ_ARGS": "-a -g -u -f sysv --no-sort -C gnu -A -S --special-syms --with-symbol-versions @@",
        "INPUT_DIR": "./test_input/nm-new/seeds/"
    },
    "readelf": {
        "AFL_CFG_PATH": "./fuzz_target_binaries/readelf_sancov_cfg",
        "FUZZ_TARGET": "./fuzz_target_binaries/readelf",
        "FUZZ_ARGS": "-a -t -g -x .text -p .data --dyn-syms --debug-dump=info -W @@",
        "INPUT_DIR": "./test_input/nm-new/seeds/"
    },
    "objdump": {
        "AFL_CFG_PATH": "./fuzz_target_binaries/objdump_sancov_cfg",
        "FUZZ_TARGET": "./fuzz_target_binaries/objdump",
        "FUZZ_ARGS": "-x -d -S -g -T -r --dwarf=info --visualize-jumps=color @@",
        "INPUT_DIR": "./test_input/nm-new/seeds/"
    },
    "size": {
        "AFL_CFG_PATH": "./fuzz_target_binaries/size_sancov_cfg",
        "FUZZ_TARGET": "./fuzz_target_binaries/size",
        "FUZZ_ARGS": "-A -d -t --common @@",
        "INPUT_DIR": "./test_input/nm-new/seeds/"
    }
}
